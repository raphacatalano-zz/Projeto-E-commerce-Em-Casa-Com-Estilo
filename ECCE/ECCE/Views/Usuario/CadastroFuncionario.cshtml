
@{
    @model tb_login;
    ViewData["Title"] = "Categorias";

    Layout = "_Layout_dashboard";
}

<h4>Cadastro de Cor</h4>

<form asp-action="SalvarFuncionario" asp-controller="Usuario">
    <div class="form-group">
        <label asp-for="Tipo"></label>
        <input type="text" class="form-control" asp-for="Tipo">
        <label asp-for="Ativo"></label>
        <input type="text" class="form-control" asp-for="Ativo">

        <input type="hidden" asp-for="CodigoLogin">

    </div>

    @Html.Raw(ViewData["Valida"])

    <button type="submit" class="btn btn-primary">Registrar</button>

</form>

